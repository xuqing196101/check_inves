					<encoding>${project.build.sourceEncoding}</encoding>
					<skip>true</skip>
					<compilerArguments>
						<extdirs>${project.basedir}/src/main/webapp/WEB-INF/lib</extdirs>
					</compilerArguments>
				</configuration>
